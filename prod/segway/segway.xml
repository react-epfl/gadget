function updateValues(){
	x[1]=parseFloat($("#x_1").val());
	x[2]=parseFloat($("#x_2").val())/180*Math.PI;
	x[3]=parseFloat($("#x_3").val());
	x[4]=parseFloat($("#x_4").val())/180*Math.PI;
